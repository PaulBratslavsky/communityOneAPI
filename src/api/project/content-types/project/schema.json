{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "name": "project"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "likesCount": {
      "type": "integer"
    },
    "developer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "projects"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "project"
    },
    "likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::like.like",
      "mappedBy": "project"
    },
    "featuredImage": {
      "type": "media",
      "allowedTypes": [
        "images"
      ],
      "multiple": false,
      "required": false
    },
    "gitUrl": {
      "type": "string"
    },
    "siteUrl": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "DRAFT",
        "PUBLISH"
      ]
    },
    "issues": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::issue.issue",
      "mappedBy": "project"
    }
  }
}
